// for loop
for (let i = 1; i <= 5; i++){
    console.log("Prabin Joshi");
}

// calcuate sum of 1 to 5 mumbers
let sum = 0;
for (let i = 1; i <= 5; i++){
    sum = sum + i;
}
console.log("sum:", sum);

// calcuate sum of 1 to n mumbers
let s = 0;
let n = 10
for (let i = 1; i <= n; i++){
    s = s + i;
}
console.log("sum:", s);

//while loop
let i = 1;
while (i <= 7) {
    console.log(i);
    i++;
}

// do-while loop
let p = 1;
do {
    console.log("i:", p);
    p++;
} while (p<=4);
